    <script>

        const questions = [

            {

                question: "What is the main chemical used along with urea to prepare Urea-Formaldehyde resin?",

                options: ["Acetic Acid", "Formaldehyde", "Sulphuric Acid"],

                answer: 1,

                explanation: "Formaldehyde is the key reactant along with urea in forming Urea-Formaldehyde resin."

            },

            {

                question: "What type of polymer is Urea-Formaldehyde resin?",

                options: ["Thermoplastic", "Elastomer", "Thermosetting Polymer"],

                answer: 2,

                explanation: "Urea-Formaldehyde is a thermosetting polymer, meaning it becomes hard and rigid when heated."

            },

            {

                question: "Which property makes Urea-Formaldehyde resin useful in electrical applications?",

                options: ["Good Adhesive Properties", "Good Electrical Insulation", "High Abrasion Resistance"],

                answer: 1,

                explanation: "Good electrical insulation makes Urea-Formaldehyde resin suitable for electrical applications."

            },

            {

                question: "What catalyst is commonly used to prepare Urea-Formaldehyde resin?",

                options: ["Sodium Hydroxide", "Sulphuric Acid", "Hydrochloric Acid"],

                answer: 1,

                explanation: "Sulphuric Acid is commonly used as a catalyst in the polymerization process."

            },

            {

                question: "What is a common use of Urea-Formaldehyde resin?",

                options: ["Food Packaging", "Textile Finishing", "Fuel Production"],

                answer: 1,

                explanation: "Urea-Formaldehyde resin is used in textile finishing to impart stiffness, crease resistance, and water repellency."

            }

        ];



        let currentQuestionIndex = 0;

        let score = 0;



        function loadQuestion() {

            const container = document.getElementById("question-container");

            const questionData = questions[currentQuestionIndex];



            let questionHTML = `<div class='question'>

                <p><strong>Question ${currentQuestionIndex + 1}:</strong> ${questionData.question}</p>`;



            questionData.options.forEach((option, index) => {

                questionHTML += `<input type='radio' name='answer' value='${index}'> ${option}<br>`;

            });



            questionHTML += `<br><button onclick='submitAnswer()'>Submit</button></div>`;



            container.innerHTML = questionHTML;

            document.getElementById("result-info").innerHTML = "";

            document.getElementById("next-button").style.display = "none";

        }



        function submitAnswer() {

            const selected = document.querySelector("input[name='answer']:checked");

            if (!selected) {

                alert("Please select an answer before submitting.");

                return;

            }



            const selectedIndex = parseInt(selected.value);

            const questionData = questions[currentQuestionIndex];

            let explanation = `<p><strong>Explanation:</strong> `;



            if (selectedIndex === questionData.answer) {

                explanation += "Correct! ";

                score++;

            } else {

                explanation += `Incorrect. The correct answer is '${questionData.options[questionData.answer]}'. `;

            }



            explanation += questionData.explanation + "</p>";

            document.getElementById("result-info").innerHTML = explanation;

            document.getElementById("next-button").style.display = "inline-block";

        }



        function nextQuestion() {

            currentQuestionIndex++;

            if (currentQuestionIndex >= questions.length) {

                showResult();

            } else {

                loadQuestion();

            }

        }



        function showResult() {

            document.getElementById("quiz-page").style.display = "none";

            document.getElementById("result-page").style.display = "block";

            document.getElementById("final-score").innerHTML =

                `<p>Your final score is <strong>${score}</strong> out of <strong>${questions.length}</strong>.</p>`;

        }



        function restartQuiz() {

            currentQuestionIndex = 0;

            score = 0;

            document.getElementById("quiz-page").style.display = "block";

            document.getElementById("result-page").style.display = "none";

            loadQuestion();

        }



        document.addEventListener("DOMContentLoaded", loadQuestion);

    </script>

</body>

</html>





