    <script>

        function showApparatusInfo(name, use, imageUrl) {

            document.getElementById('apparatusName').textContent = name;

            document.getElementById('apparatusUse').textContent = use;

           /* document.getElementById('apparatusImage').src = imageUrl; */

            document.getElementById('info-panel').style.display = 'block';

            window.scrollTo({top: 0, behavior: 'smooth'});

            pronounceText(name); // Automatically pronounce the name

        }





        function pronounceText(text) {

            const utterance = new SpeechSynthesisUtterance(text);

            utterance.rate = 0.9; // Adjust speed if needed

            speechSynthesis.speak(utterance);

        }

    </script>

