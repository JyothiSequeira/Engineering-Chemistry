    let index = 0;

    let typingSpeed = 50; // Speed of typing effect



    function typeMessage(text, i = 0) {

        if (i < text.length) {

            document.getElementById("teachers_saying").innerText = text.substring(0, i + 1);

            setTimeout(() => typeMessage(text, i + 1), typingSpeed);

